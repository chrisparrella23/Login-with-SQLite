   <children>
      <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="248.0" prefWidth="248.0">
         <children>
            <Label layoutX="54.0" layoutY="27.0" text="Change Password">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="newPassword" layoutX="50.0" layoutY="138.0" promptText="New Password" />
            <Button layoutX="97.0" layoutY="186.0" mnemonicParsing="false" onAction="#changePassword" text="Submit" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
