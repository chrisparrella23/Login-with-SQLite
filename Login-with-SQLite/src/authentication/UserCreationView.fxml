<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="authentication.UserCreationController">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="248.0">
         <children>
            <TextField fx:id="username" layoutX="50.0" layoutY="50.0" promptText="Username" />
            <TextField fx:id="password" layoutX="50.0" layoutY="81.0" promptText="Password" />
            <TextField fx:id="firstName" layoutX="50.0" layoutY="112.0" promptText="First Name" />
            <TextField fx:id="lastName" layoutX="50.0" layoutY="144.0" promptText="Last Name" />
            <TextField fx:id="phone" layoutX="50.0" layoutY="176.0" promptText="Phone" />
            <TextField fx:id="email" layoutX="50.0" layoutY="208.0" promptText="Email" />
            <Label layoutX="93.0" layoutY="14.0" text="Sign Up">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button layoutX="95.0" layoutY="244.0" mnemonicParsing="false" onAction="#createUser" text="Sign Up" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
